div.wpcf7 input[type="text"], div.wpcf7 input[type="email"], div.wpcf7 textarea {
	width:400px;
	//height:40px;
	font-size: 150%;
	border: 1px #ccc solid;
}

/*RadioとText*/
div.wpcf7 .your-sex label {
	cursor       : pointer;
	position     : relative;
	padding-left : 5px;
	margin-right : 20px;
	overflow     : hidden;
	padding-left : 20px;
	display      : inline-block;
}
div.wpcf7 .your-sex label:before {
	position      : absolute;
	width         : 15px;
	height        : 15px;
	border        : 1px solid #ccc;
	border-radius : 50%;
	left          : 0px;
	top           : 2px;
	content       : '';
	z-index       : 3;
}
div.wpcf7 .your-sex label:after {
	content          : '';
	position         : absolute;
	width            : 11px;
	height           : 11px;
	border-radius    : 100%;
	left             : 3px;
	top              : 5px;
	background-color : #c83539;
	z-index          : 1;
}
div.wpcf7 .your-sex label input[type="radio"] {
	-moz-appearance: none;
	-webkit-appearance: none;
	position   : absolute;
	z-index    : 2;
	width      : 17px;
	height     : 17px;
	left       : -23px;
	top        : 2px;
	margin     : 0px;
	box-shadow : 20px -1px #f0f0f0;
}
div.wpcf7 .your-sex label input[type="radio"]:checked {
	box-shadow : none;
}
div.wpcf7 .your-sex label input[type="radio"]:focus {
	opacity    : 0.2;
	box-shadow : 20px -1px #f0f0f0;
}

div.wpcf7 input[type="submit"] {
	width:250px;
	height:40px;
	font-size: 100%;
	color:#fff;
	background-color : #c83539;
	border: 1px #c83539 solid;
	border-radius:3px;
	text-align:center;
}

div.wpcf7 p:first-child {
	border-top: 2px #ccc dotted;
}


